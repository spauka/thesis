@article {Nowack1430,
	author = {Nowack, K. C. and Koppens, F. H. L. and Nazarov, Yu. V. and Vandersypen, L. M. K.},
	title = {Coherent Control of a Single Electron Spin with Electric Fields},
	volume = {318},
	number = {5855},
	pages = {1430--1433},
	year = {2007},
	doi = {10.1126/science.1148092},
	publisher = {American Association for the Advancement of Science},
	abstract = {Manipulation of single spins is essential for spin-based quantum information processing. Electrical control instead of magnetic control is particularly appealing for this purpose, because electric fields are easy to generate locally on-chip. We experimentally realized coherent control of a single-electron spin in a quantum dot using an oscillating electric field generated by a local gate. The electric field induced coherent transitions (Rabi oscillations) between spin-up and spin-down with 90{\textdegree} rotations as fast as \~{}55 nanoseconds. Our analysis indicated that the electrically induced spin transitions were mediated by the spin-orbit interaction. Taken together with the recently demonstrated coherent exchange of two neighboring spins, our results establish the feasibility of fully electrical manipulation of spin qubits.},
	issn = {0036-8075},
	URL = {https://science.sciencemag.org/content/318/5855/1430},
	eprint = {https://science.sciencemag.org/content/318/5855/1430.full.pdf},
	journal = {Science}
}

