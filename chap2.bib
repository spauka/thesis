@article{YOU2005121,
title = "A 5–10{GHz} {SiGe} {BiCMOS} {FPGA} with new configurable logic block",
journal = "Microprocessors and Microsystems",
volume = "29",
number = "2",
pages = "121 - 131",
year = "2005",
note = "Special Issue on FPGA Tools and Techniques",
issn = "0141-9331",
doi = "10.1016/j.micpro.2004.06.008",
url = "http://www.sciencedirect.com/science/article/pii/S0141933104000870",
author = "Chao You and Jong-Ru Guo and Russell P. Kraft and Michael Chu and Peter Curran and Kuan Zhou and Bryan Goda and John F. McDonald",
keywords = "Field programmable gate array, Gate-array, High-performance, Low-power-design, SiGe, Programmable logic array, Reconfigurable-system, Current mode logic",
abstract = "This paper presents a new multiplexer based FPGA, which can operate at a clock frequency of 5–10GHz. Redundant switches on the original signal paths are removed improving the performance. The configurable logic blocks (CLBs) power is greatly reduced by using a revised multiplexer structure and turning off unused cells dynamically. More routing capabilities are provided with more inputs/outputs in each direction than similar designs. A chip consisting of four FPGA ring oscillators was fabricated. The Spice simulation results and chip measurements are presented."
}

