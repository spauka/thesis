@article{RELLING1989380,
title = "Solid state diffusion in GaAs/AuGe/Ni and GaAs/Ni/AuGe/Ni ohmic contacts",
journal = "Applied Surface Science",
volume = "35",
number = "3",
pages = "380 - 387",
year = "1989",
issn = "0169-4332",
doi = "https://doi.org/10.1016/0169-4332(89)90020-2",
url = "http://www.sciencedirect.com/science/article/pii/0169433289900202",
author = "E. Relling and A.P. Botha",
abstract = "Auger depth profiling and Rutherford backscattering spectroscopy have been used to study GaAs/AuGe/Ni and GaAs/Ni/AuGe/Ni ohmic contacts as a function of annealing temperature. Results indicate that the incorporation of Ge into GaAs occurs via solid state diffusion. The formation of compounds and reactions which occur with varying the annealing temperature are discussed. It is found that the Ge distribution within the metallization layer differs in the two systems studied. The interfacial layer in the GaAs/Ni/AuGe/Ni system prevents Ge from diffusing away from the GaAs interface, resulting in a greater availability of Ge at the GaAs interface."
}

